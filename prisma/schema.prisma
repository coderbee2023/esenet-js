    datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
 
generator client {
  provider = "prisma-client-js"
}
 
model registration{
    id  String @id @default(cuid())
    lastname String
    firstname String
    email String
    phone String
    university String
    heardAboutUs String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}